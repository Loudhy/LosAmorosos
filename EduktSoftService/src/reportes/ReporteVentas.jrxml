	<queryString>
		<![CDATA[SELECT E.ID_EMPLEADO, E.NOMBRE_EMPLEADO, E.APELLIDO_PATERNO, E.APELLIDO_MATERNO, E.DNI_EMPLEADO, E.CORREO_EMPLEADO, E.FOTO , ROUND(SUM(P.TOTAL_PEDIDO),2)AS VENTA FROM PEDIDO P,EMPLEADO E, CLIENTE_VENDEDOR CV WHERE E.ID_AREA = 3 AND E.ID_EMPLEADO = CV.ID_VENDEDOR AND CV.ID_CLIENTE_VENDEDOR = P.ID_CLIENTE_VENDEDOR
AND P.ESTADO_PEDIDO = 'Pagado' AND DATE($P{FECHA_INI})<=P.FECHA_REGISTRO  AND DATE($P{FECHA_FIN})>= P.FECHA_REGISTRO AND E.ACTIVE = 1
GROUP BY E.ID_EMPLEADO
ORDER BY VENTA;]]>
