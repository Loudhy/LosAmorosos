	<queryString>
		<![CDATA[SELECT E.ID_EMPLEADO, E.NOMBRE_EMPLEADO, E.APELLIDO_PATERNO, E.APELLIDO_MATERNO, E.DNI_EMPLEADO,
E.CORREO_EMPLEADO , E.FOTO, TRUNCATE(SUM(P.MONTO_FACTURADO),2)AS VENTA FROM PEDIDO P,EMPLEADO E,
CLIENTE_VENDEDOR CV WHERE E.ID_AREA = 3 AND E.ID_EMPLEADO = CV.ID_VENDEDOR AND
CV.ID_CLIENTE_VENDEDOR = P.ID_CLIENTE_VENDEDOR AND P.ESTADO_PEDIDO = 'Pagado' AND
(P.FECHA_PAGO BETWEEN $P{FECHA_INI} AND $P{FECHA_FIN}) AND E.ACTIVE = 1
GROUP BY E.ID_EMPLEADO
ORDER BY VENTA;]]>
	</queryString>
