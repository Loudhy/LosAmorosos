	<subDataset name="New Dataset 1" uuid="a4ce56d3-b8f4-472e-9063-8667953b0535">
		<queryString language="SQL">
			<![CDATA[SELECT C.RAZON_SOCIAL,C.RUC,C.DIRECCION,C.TELEFONO_CLIENTE,C.PUNTOS,
P.TOTAL_PEDIDO,P.FECHA_REGISTRO,LP.CANTIDAD_A_ATENDER,LP.FECHA_ATENCION,LP.SUBTOTAL,
PR.NOMBRE_PRODUCTO,PR.PRECIO_UNITARIO
FROM PEDIDO P
INNER JOIN CLIENTE_VENDEDOR CV ON P.ID_CLIENTE_VENDEDOR = CV.ID_CLIENTE_VENDEDOR
INNER JOIN CLIENTE C ON CV.ID_CLIENTE = C.ID_CLIENTE
INNER JOIN LINEA_PEDIDO LP ON LP.ID_PEDIDO = P.ID_PEDIDO
INNER JOIN PRODUCTO PR ON LP.ID_PRODUCTO = PR.ID_PRODUCTO
WHERE P.ID_PEDIDO = 1;]]>
		</queryString>
