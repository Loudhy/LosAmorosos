	<parameter name="ID_PEDIDO" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="LOGO_DIR" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT C.RAZON_SOCIAL,C.RUC,C.DIRECCION,C.TELEFONO_CLIENTE,C.PUNTOS,P.ID_PEDIDO,
P.TOTAL_PEDIDO,P.FECHA_REGISTRO,LP.CANTIDAD_A_ATENDER,LP.FECHA_ATENCION,LP.SUBTOTAL,
PR.NOMBRE_PRODUCTO,PR.PRECIO_UNITARIO,LP.ESTADO_LINEA_PEDIDO,(SELECT TRUNCATE(SUM(LP.SUBTOTAL),2) 
						FROM PEDIDO P
                                                              INNER JOIN LINEA_PEDIDO LP ON P.ID_PEDIDO = LP.ID_PEDIDO
						WHERE P.ID_PEDIDO = $P{ID_PEDIDO} AND 
 						LP.ESTADO_LINEA_PEDIDO = 'Aceptado') AS SUMA_TOTAL, IF(C.PUNTOS>70,"5%", "0%"), P.MONTO_FACTURADO
FROM PEDIDO P
INNER JOIN CLIENTE_VENDEDOR CV ON P.ID_CLIENTE_VENDEDOR = CV.ID_CLIENTE_VENDEDOR
INNER JOIN CLIENTE C ON CV.ID_CLIENTE = C.ID_CLIENTE
INNER JOIN LINEA_PEDIDO LP ON LP.ID_PEDIDO = P.ID_PEDIDO
INNER JOIN PRODUCTO PR ON LP.ID_PRODUCTO = PR.ID_PRODUCTO

WHERE P.ID_PEDIDO = $P{ID_PEDIDO} AND LP.ESTADO_LINEA_PEDIDO = 'Aceptado';
]]>
	</queryString>
