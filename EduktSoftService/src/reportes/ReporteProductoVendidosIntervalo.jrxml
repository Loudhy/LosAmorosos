	<parameter name="LOGO_DIR" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT PR.FOTO,PR.ID_PRODUCTO,SUM(LP.CANTIDAD_A_ATENDER) AS CANTIDAD_AGRUPADA,P.ID_PEDIDO,P.TOTAL_PEDIDO,LP.CANTIDAD,LP.SUBTOTAL,LP.CANTIDAD_A_ATENDER,PR.NOMBRE_PRODUCTO,PR.PRECIO_UNITARIO,PR.DESCRIPCION
FROM PEDIDO P
INNER JOIN LINEA_PEDIDO LP ON P.ID_PEDIDO = LP.ID_PEDIDO
INNER JOIN PRODUCTO PR ON LP.ID_PRODUCTO = PR.ID_PRODUCTO
WHERE (P.ESTADO_PEDIDO = 'Pagado') AND (P.FECHA_REGISTRO BETWEEN $P{FECHA_INICIO} AND $P{FECHA_FIN}) 
GROUP BY PR.ID_PRODUCTO
ORDER BY PR.ID_PRODUCTO;]]>
	</queryString>
